<%- include('common/header') %>
<div class="container">
	<div class="row">
		<div class="col-md-4"></div>
		<div class="col-md-4">
			<form role="form" action="/signin" method="post">
			  <div class="form-group">
			    <label for="username">用户名:</label>
			    <input name="uname" type="text" class="form-control" id="username" placeholder="your name">
			  </div>
			  <div class="form-group">
			    <label for="passwd">密码</label>
			    <input name="passwd" type="password" class="form-control" id="passwd" placeholder="Password">
			  </div>
			  <button type="submit" class="btn btn-success btn-block">登录</button>
			</form>
		</div>
		<div class="col-md-4"></div>
	</div>
</div>
<%- include('common/footer') %>